{
    "cSpell.enableFiletypes": ["latex"],
    "cSpell.language": "en,pl",
    "cSpell.words": [
        "biblatex",
        "Cython",
        "Cythona",
        "Cythonie",
        "Numba",
        "otwartoźródłowych",
        "Rossum",
        "TIOBE",
        "transpilować",
        "uruchomieniowe",
        "wzwiązku"
    ],
    "[latex]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.wrappingIndent": "same",
        "editor.wordWrapColumn": 110,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "indentRainbow.ignoreErrorLanguages": ["bibtex", "markdown", "latex", "tex"]
}
